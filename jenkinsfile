pipeline {
    agent any
    stages {
        stage('build and install') {
            steps {
             image 'justb4/jmeter:5.5'

            }
        }

        stage('run jmeter tests') {
            steps {
                sh 'jmeter -n -t src/test/jmeter/Plandetestdepartements.jmx'
            }
        }
    }
}
